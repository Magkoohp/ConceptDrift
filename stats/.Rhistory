install.packages(c('SPARQL','igraph','network','ergm'),dependencies=TRUE)
library(SPARQL)
endpoint <- "http://kizokumaru.dyndns.org:3030/cedar/sparql"
options <- NULL
q <- "PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX d2s: <http://www.data2semantics.org/core/> 
SELECT ?label
FROM <http://www.cedar-project.nl/data/1859>
WHERE {
?s a skos:Concept ;
d2s:value ?value .
?value skos:prefLabel ?label .
} ORDER BY ?value"
res <- SPARQL(endpoint,q,)$results
res
res$label
res$label.2
resl <- as.vector(res)
resl
as.array(res)
as.list(res)
rl <- c(res)
rl
rownames(dfx) <- res
